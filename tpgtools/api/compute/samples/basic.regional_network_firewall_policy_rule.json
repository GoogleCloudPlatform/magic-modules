{
  "priority": 9000,
  "project": "{{project}}",
  "location": "{{region}}",
  "firewallPolicy": "projects/{{project}}/regions/{{region}}/firewallPolicies/{{ref:basic.regional_network_firewall_policy.json:name}}",
  "description": "sample regional policy rule",
  "name": "rule9000-allow-probe",
  "match": {
    "layer4Configs": [
      {
        "ipProtocol": "tcp",
        "ports": ["123"]
      }
    ],
    "destIPRanges": ["10.100.0.1/32"]
  },
  "enableLogging" : false,
  "targetSecureTags": [{"name": "tagValues/12345"}],
  "action": "deny",
  "direction": "EGRESS",
  "disabled" : false
}
