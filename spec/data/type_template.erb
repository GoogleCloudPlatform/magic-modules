<% autogen_exception -%>
type: <%= object.out_name %>
<% unless object.parameters.nil? -%>
<% object.parameters.each do |param| -%>
parameter: <%= param.out_name -%> (<%= param.type -%>)
<% end -%>
<% end -%>
<% object.properties.each do |prop| -%>
property: <%= prop.out_name -%> (<%= prop.type -%>)
<% end -%>
