resource: 'google_compute_attached_disk'
generation_type: 'handwritten'
api_service_name: 'compute.googleapis.com'
{{- if ne $.TargetVersionName "ga" }}
api_version: 'beta'
{{- else }}
api_version: 'v1'
{{- end }}
api_resource_type_kind: 'Instance'
fields:
  - api_field: 'disks.deviceName'
    field: 'device_name'
  - api_field: 'disks.source'
    field: 'disk'
  - field: 'instance'
  - api_field: 'disks.interface'
    field: 'interface'
  - api_field: 'disks.mode'
    field: 'mode'
  - field: 'project'
  - api_field: 'zone'
