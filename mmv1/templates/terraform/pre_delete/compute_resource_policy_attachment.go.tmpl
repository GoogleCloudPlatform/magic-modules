obj = make(map[string]interface{})

zone, err := tpgresource.GetZone(d, config)
if err != nil {
	return err
}

region := tpgresource.GetRegionFromZone(zone)
if region == "" {
	return fmt.Errorf("invalid zone %q", zone)
}

