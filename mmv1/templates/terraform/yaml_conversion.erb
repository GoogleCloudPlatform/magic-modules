<%    end -%>
<%    unless object.iam_policy.iam_policy_version.nil? -%>
  iam_policy_version: '<%= object.iam_policy.iam_policy_version %>'
<%    end -%>
<%    unless object.iam_policy.min_version.nil? -%>
