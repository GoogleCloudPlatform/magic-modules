<%  end -%>
<%  unless object.update_mask.nil? -%>
update_mask: <%= object.update_mask %>
<%  end -%>
<%  unless object.read_verb.to_s == 'GET' -%>
read_verb: '<%= object.read_verb.to_s %>'
<%  end -%>
<%  unless object.read_query_params.nil? %>
