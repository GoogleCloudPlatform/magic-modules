// diffsuppress for enableHotTierAutoResize field
func enableHotTierAutoResizeDiffSuppress(_, old, new string, d *schema.ResourceData) bool {
    allowAutoTiering, ok := d.GetOk("allowAutoTiering")
    if ok {
		if allowAutoTieringBool, isBool := allowAutoTiering.(bool); isBool {
			if !allowAutoTieringBool {
				// If allowAutoTiering is true, suppress the diff for enableHotTierAutoResize.
				return false
			}
		}
	}
    oldBool, errOld := strconv.ParseBool(old)
	newBool, errNew := strconv.ParseBool(new)
    if errOld != nil || errNew != nil {
		return false
	}
	return oldBool == newBool
}