const datasetIdRegexp = `^deleted:`

func validateDeletedPrincipals(v interface{}, k string) (ws []string, errors []error) {
    value := v.(string)
    if regexp.MustCompile(datasetIdRegexp).MatchString(value) {
       errors = append(errors, fmt.Errorf(
            "Terraform does not support IAM policies for deleted principals", k))
    }

    return
}
