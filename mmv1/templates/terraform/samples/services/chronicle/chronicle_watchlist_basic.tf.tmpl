resource "google_chronicle_watchlist" "{{$.PrimaryResourceId}}" {
  location = "us"
  instance = "{{index $.TestEnvVars "chronicle_id"}}"
  watchlist_id = "{{index $.PrefixedVars "watchlist_id"}}"
  description = "{{index $.PrefixedVars "description"}}"
  display_name = "{{index $.PrefixedVars "name"}}"
  multiplying_factor = 1
  entity_population_mechanism {
    manual {

    }
  }
  watchlist_user_preferences {
    pinned = true
  }
}
