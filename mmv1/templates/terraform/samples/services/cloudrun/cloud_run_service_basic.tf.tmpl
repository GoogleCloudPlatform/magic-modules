resource "google_cloud_run_service" "{{$.PrimaryResourceId}}" {
  name     = "{{index $.PrefixedVars "cloud_run_service_name"}}"
  location = "us-central1"

  template {
    spec {
      containers {
        image = "us-docker.pkg.dev/cloudrun/container/hello"
      }
    }
  }

  traffic {
    percent         = 100
    latest_revision = true
  }
}
