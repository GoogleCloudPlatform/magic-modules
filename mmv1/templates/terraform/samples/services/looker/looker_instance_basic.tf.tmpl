resource "google_looker_instance" "{{$.PrimaryResourceId}}" {
  name              = "{{index $.PrefixedVars "instance_name"}}"
  platform_edition  = "LOOKER_CORE_STANDARD_ANNUAL"
  region            = "us-central1"
  oauth_config {
    client_id = "{{index $.PrefixedVars "client_id"}}"
    client_secret = "{{index $.PrefixedVars "client_secret"}}"
  }
  deletion_policy = "DEFAULT"
}
