resource "google_dataproc_session_template" "{{$.PrimaryResourceId}}" {
    name     = "projects/{{index $.TestEnvVars "project_name"}}/locations/us-central1/sessionTemplates/{{index $.PrefixedVars "name"}}"
    location = "us-central1"
    labels   = {"session_template_test": "terraform"}

    runtime_config {
      properties    = { "spark.dynamicAllocation.enabled": "false", "spark.executor.instances": "2" }
    }

    environment_config {
      execution_config {
        subnetwork_uri = "{{index $.PrefixedVars "subnetwork_name"}}"
        idle_ttl       = "3600s"
        network_tags   = ["tag1"]
        authentication_config {
          user_workload_authentication_type = "END_USER_CREDENTIALS"
        }
      }
    }

    jupyter_session {
      kernel       = "PYTHON"
      display_name = "tf python kernel"
    }
}
