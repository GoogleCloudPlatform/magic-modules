resource "google_database_migration_service_connection_profile" "{{$.PrimaryResourceId}}" {
  location = "us-central1"
  connection_profile_id = "{{index $.PrefixedVars "profile"}}"
  display_name = "{{index $.PrefixedVars "profile"}}_display"
  labels = { 
    foo = "bar" 
  }
