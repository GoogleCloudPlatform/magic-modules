resource "google_oracle_database_odb_network" "{{$.PrimaryResourceId}}"{
  odb_network_id = "{{index $.PrefixedVars "odb_network_id"}}"
  location = "us-west3"
  project = "{{index $.PrefixedVars "project"}}"
  network = data.google_compute_network.default.id
  labels = {
    terraform_created = "true"
  }
  deletion_protection = "{{index $.PrefixedVars "deletion_protection"}}"
}

data "google_compute_network" "default" {
  name     = "new"
  project = "{{index $.PrefixedVars "project"}}"
}