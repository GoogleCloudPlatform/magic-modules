resource "google_dns_managed_zone" "{{$.PrimaryResourceId}}" {
  name        = "{{index $.PrefixedVars "zone_name"}}"
  dns_name    = "{{index $.PrefixedVars "dns_name"}}"
  description = "Example cloud logging enabled DNS zone"
  labels = {
    foo = "bar"
  }

  cloud_logging_config {
    enable_logging = true
  }
}