if p, ok := d.GetOk("desired_state"); ok {
    if d.Get("type") == "SUBORDINATE" {
        return fmt.Errorf("`desired_state` can not be specified when creating a SUBORDINATE CA")
    }
    if p.(string) != "STAGED" && p.(string) != "ENABLED" {
        return fmt.Errorf("`desired_state` can only be set to `STAGED` or `ENABLED` when creating a new CertificateAuthority")
    }
}
