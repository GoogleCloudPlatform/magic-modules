nsRaw_pre := d.Get("namespace").([]interface{})
	ns_pre := make([]string, len(nsRaw_pre))
	for i, v := range nsRaw_pre {
		ns_pre[i] = v.(string)
	}
    encodedNs_pre := encodeNamespace(ns_pre)
    if err := d.Set("namespace_id", encodedNs_pre); err != nil {
        return fmt.Errorf("Error setting namespace_id in pre-create: %s", err)
    }
    
    // variables 'project' and 'err' are already declared in the base template's Create function
    project, err = tpgresource.GetProject(d, config)
    if err != nil {
        return err
    }
    catalog_pre := d.Get("catalog").(string)
    
    // Set ID early to ensure consistency. 
    // Avoid naming this 'id' to prevent conflict with the 'id' package.
    resourceID_pre := fmt.Sprintf("projects/%s/catalogs/%s/namespaces/%s", project, catalog_pre, encodedNs_pre)
    d.SetId(resourceID_pre)