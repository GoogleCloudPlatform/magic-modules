{{- define "expandResourceRef" }}
{{- if contains $.Property.ResourceRef.BaseUrl "{{region}}" -}}
tpgresource.ParseRegionalFieldValue("{{$.Property.ResourceType}}", {{$.VarName}}, "project", "region", "zone", d, config, true)
{{- else if contains $.Property.ResourceRef.BaseUrl "{{zone}}" -}}
tpgresource.ParseZonalFieldValue("{{$.Property.ResourceType}}", {{$.VarName}}, "project", "zone", d, config, true)
{{- else -}}
tpgresource.ParseGlobalFieldValue("{{$.Property.ResourceType}}", {{$.VarName}}, "project", d, config, true)
{{- end -}}
{{- end}}
