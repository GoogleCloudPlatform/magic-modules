	// connection_id is needed to qualify the URL but cannot be sent in the body
	delete(obj, "connection_id")
	
	// Remove passwordWoVersion as it should not be sent in requests
	if cloudSql, ok := obj["cloudSql"].(map[string]interface{}); ok {
		if credential, ok := cloudSql["credential"].(map[string]interface{}); ok {
			delete(credential, "passwordWoVersion")
		}
	}
	
	return obj, nil