	old, new := d.GetChange("autoscaling_config")
	oldSlice := old.([]interface{})
	newSlice := new.([]interface{})
	if len(oldSlice) == 0 || len(newSlice) == 0 {
		updateMask = append(updateMask, "autoscalingConfig")
