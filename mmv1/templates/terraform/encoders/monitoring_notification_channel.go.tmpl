labelmap, ok := obj["labels"]
if !ok {
	labelmap = make(map[string]string)
}

var labels map[string]string
labels = labelmap.(map[string]string)

for _, sl := range sensitiveLabels {
	if auth, _ := d.GetOkExists("sensitive_labels.0." + sl); auth != "" {
		labels[sl] = auth.(string)
	}
}

{{- if ne $.Compiler "terraformgoogleconversion-codegen" }}
	for _, sp := range sensitiveWoLabels {
		if auth, _ := d.GetRawConfigAt(cty.GetAttrPath("sensitive_labels_wo").IndexInt(0).GetAttr(sp)); !auth.IsNull() && auth.Type().Equals(cty.String) {
			sp = sp[:len(sp)-3] // _wo is convention for write-only labels and are removed here
			labels[sp] = auth.AsString()
		}
	}
{{- end }}

obj["labels"] = labels

return obj, nil