resource "google_network_security_server_tls_policy" "<%= ctx[:primary_resource_id] %>" {
  name                   = "<%= ctx[:vars]['resource_name'] %>"
  labels                 = {
    foo = "bar"
  }
  description            = "my description"
  location               = "global"
  allow_open             = "false"
  server_certificate {
    grpc_endpoint {
        target_uri = "unix:mypath"
      }
  }
}

