resource "google_data_loss_prevention_deidentify_template" "<%= ctx[:primary_resource_id] %>" {
  parent = "projects/<%= ctx[:test_env_vars]['project'] %>"
  template_id = "<%= ctx[:vars]['name'] %>"

  deidentify_config {
    info_type_transformations {
      transformations {
        info_types {
          name = "PHONE_NUMBER"
        }
        info_types {
          name = "AGE"
        }

        primitive_transformation {
          replace_config {
            new_value {
              integer_value = 9
            }
          }
        }
      }
    }
  }
}
