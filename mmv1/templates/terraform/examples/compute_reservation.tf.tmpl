
resource "google_compute_reservation" "<%= ctx[:primary_resource_id] %>" {
  name = "<%= ctx[:vars]['gce_reservation_local'] %>"
  zone = "us-central1-c"
  project = "<%= ctx[:test_env_vars]['project'] %>"

  share_settings {
    share_type = "LOCAL"
  }

  specific_reservation {
    count = 1
    instance_properties {
      machine_type     = "n2-standard-2"
    }
  }
}

