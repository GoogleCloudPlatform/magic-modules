resource "google_ces_app" "my-app" {
    location     = "us"
    display_name = "{{index $.Vars "app_display_name"}}"
    app_id       = "{{index $.Vars "app_id"}}"
}
resource "google_ces_example" "{{$.PrimaryResourceId}}" {
    location     = "us"
    display_name = "{{index $.Vars "example_display_name"}}"
    app          = google_ces_app.my-app.name
    example_id   = "{{index $.Vars "example_id"}}"
    description  = "example description"
    entry_agent  = "projects/example-project/locations/us/apps/example-app/agents/example-agent"
    messages {
        chunks {
            agent_transfer {
                target_agent = "projects/example-project/locations/us/apps/example-app/agents/example-agent2"
            }
        }
        chunks {
            image {
                mime_type = "image/png"
                data = base64encode("This is some fake image binary data.")
            }
        }
        chunks {
            text = "text_data"
        }
        chunks {
            tool_call {
                args = jsonencode({
                    arg1 = "val1"
                    arg2 = "val2"
                })
                id = "tool_call_id"
                tool = "projects/example-project/locations/us/apps/example-app/tools/example-tool"
            }
        }
        chunks {
            tool_call {
                args = jsonencode({
                    arg1 = "val1"
                    arg2 = "val2"
                })
                id = "tool_call_id1"
                toolset_tool {
                    toolset = "projects/example-project/locations/us/apps/example-app/toolsets/example-toolset"
                    tool_predicate {
                        connector_tool_predicate {
                            action {
                                input_fields = ["entity_input_field1","entity_input_field2"]
                                output_fields = ["entity_output_field1","entity_output_field2"]
                                connection_action_id = "connection_action_id"
                            }
                        }
                    }
                }
            }
        }
        chunks {
            tool_call {
                args = jsonencode({
                    arg1 = "val1"
                    arg2 = "val2"
                })
                id = "tool_call_id2"
                toolset_tool {
                    toolset = "projects/example-project/locations/us/apps/example-app/toolsets/example-toolset"
                    tool_predicate {
                        connector_tool_predicate {
                            action {
                                input_fields = ["entity_input_field1","entity_input_field2"]
                                output_fields = ["entity_output_field1","entity_output_field2"]
                                entity_operation {
                                    entity_id = "entity_id"
