resource "google_data_loss_prevention_discovery_config" "<%= ctx[:primary_resource_id] %>" {
	parent = "organizations/<%= ctx[:test_env_vars]['organization'] %>/locations/us"

    targets {
        big_query_target {
            filter {
                other_tables {}
            }
        }
    }
    org_config {
        project_id = "<%= ctx[:test_env_vars]['project'] %>"
        location {
            organization_id = "<%= ctx[:test_env_vars]['organization'] %>"
        }
    }
    inspect_templates = ["projects/%{project}/locations/us/inspectTemplates/test"]    
    status = "RUNNING"
}
