resource "google_oracle_database_db_system" "{{$.PrimaryResourceId}}"{
    db_system_id = "{{index $.Vars "db_system_id"}}"
    display_name = "{{index $.Vars "db_system_id"}} displayname"
    location = "europe-west2"
    project = "{{index $.Vars "project"}}"
    odb_network = "{{index $.Vars "odb_network"}}"
    odb_subnet = "{{index $.Vars "odb_subnet"}}"
    properties {
        ssh_public_keys = ["ssh-rsa  nobody@google.com"]
        compute_count = "4"
        hostname_prefix = "hostname1"
        shape = "VM.Standard.x86"
        initial_data_storage_size_gb = "256"
        database_edition = "STANDARD_EDITION"
        license_model = "LICENSE_INCLUDED"
        time_zone {
            id = "UTC"
        }
        db_home {
            db_version = "19.0.0.0"
            database {
                admin_password = "ABcde_1#234"
                database_id = "{{index $.Vars "database_id"}}"
                db_name = "db"
            }
        }
    }
    deletion_protection = "{{index $.Vars "deletion_protection"}}"
}



