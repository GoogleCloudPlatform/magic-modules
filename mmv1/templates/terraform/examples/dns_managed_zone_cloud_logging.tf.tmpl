resource "google_dns_managed_zone" "<%= ctx[:primary_resource_id] %>" {
  name        = "<%= ctx[:vars]['zone_name'] %>"
  dns_name    = "services.example.com."
  description = "Example cloud logging enabled DNS zone"
  labels = {
    foo = "bar"
  }

  cloud_logging_config {
    enable_logging = true
  }
}