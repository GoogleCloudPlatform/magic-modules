data "google_tpu_v2_runtime_versions" "available" {
  provider = google-beta
}

resource "google_tpu_v2_vm" "<%= ctx[:primary_resource_id] %>" {
  provider = google-beta

  name = "<%= ctx[:vars]["vm_name"] %>"
  zone = "us-central1-c"

  runtime_version = "<%= ctx[:test_env_vars]["vm_runtime_version"] %>"
}
