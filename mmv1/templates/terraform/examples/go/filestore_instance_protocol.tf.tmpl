resource "google_filestore_instance" "{{$.PrimaryResourceId}}" {
  provider = google-beta
  name     = "{{index $.Vars "instance_name"}}"
  location = "us-central1"
  tier     = "ENTERPRISE"
  protocol = "NFS_V4_1"

  file_shares {
    capacity_gb = 1024
    name        = "share1"
  }

  networks {
    network = "default"
    modes   = ["MODE_IPV4"]
  }

}
