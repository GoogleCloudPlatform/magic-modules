# [START cloud_sql_mysql_instance_public_ip]
resource "google_sql_database_instance" "<%= ctx[:primary_resource_id] %>" {
  database_version = "MYSQL_5_7"
  name             = "<%= ctx[:vars]['mysql_public_ip_instance_name'] %>"
  region           = "asia-southeast2"
  settings {
    availability_type = "ZONAL"
    disk_size         = 100
    disk_type         = "PD_SSD"
    ip_configuration {
      authorized_networks {
        name  = "test-net-3"
        value = "203.0.113.0/24"
      }
      ipv4_enabled = true
    }
    tier = "db-custom-4-26624"
  }
}
# [END cloud_sql_mysql_instance_public_ip]
