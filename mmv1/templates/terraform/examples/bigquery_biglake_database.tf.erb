resource "google_bigquery_biglake_catalog" "default" {
    name = "<%= ctx[:vars]['catalog_name'] %>"
}

resource "google_bigquery_biglake_database" "<%= ctx[:vars][':primary_resource_name'] %>" {
    name = "<%= ctx[:vars]['database_name'] %>"
    catalog = google_bigquery_biglake_catalog.default.name
}
