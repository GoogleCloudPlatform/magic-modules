resource "google_biglake_catalog" "default" {
    catalog_id = "<%= ctx[:vars]['catalog_id'] %>"
    # Hard code to avoid invalid random id suffix
    location = "US"
}

resource "google_biglake_database" "<%= ctx[:vars][':primary_resource_name'] %>" {
    database_id = "<%= ctx[:vars]['database_id'] %>"
    catalog_id = google_biglake_catalog.default.catalog_id
    # Hard code to avoid invalid random id suffix
    location = google_biglake_catalog.default.location
}
