resource "google_kms_ekm_connection" "<%= ctx[:primary_resource_id] %>" {
  name            	= "<%= ctx[:vars]['ekmconnection_name'] %>"
  location		= "us-central1"
  key_management_mode 	= "MANUAL"
  service_resolvers  	{
      service_directory_service  = "<%= ctx[:vars]['servicedirectoryservice'] %>"
      hostname 			 = "<%= ctx[:vars]['hostname'] %>"
      server_certificates        {
      		raw_der	= "<%= ctx[:vars]['raw_der'] %>"
      	}
    }
}
