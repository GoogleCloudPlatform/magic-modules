resource "google_cloudbuild_trigger" "{{$.PrimaryResourceId}}" {
  location = "europe-west4"

  developer_connect_event_config {
    git_repository_link = "projects/my-project/locations/europe-west4/connections/my-connection/gitRepositoryLinks/YOUR_GIT_REPOSITORY_LINK_NAME"
    push {
      branch = "feature-.*"
    }
  }

  filename = "cloudbuild.yaml"
}