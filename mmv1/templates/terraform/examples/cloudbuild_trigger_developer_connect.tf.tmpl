resource "google_cloudbuild_trigger" "{{$.PrimaryResourceId}}" {
  location = "us-central1"

  developer_connect_event_config {
    git_repository_link = "projects/ci-test-project-188019/locations/us-central1/connections/my-connection/gitRepositoryLinks/YOUR_GIT_REPOSITORY_LINK_NAME"
    push {
      branch = "feature-.*"
    }
  }

  filename = "cloudbuild.yaml"
}