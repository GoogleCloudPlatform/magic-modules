resource "google_cloudbuild_trigger" "{{$.PrimaryResourceId}}" {
  location = "us-central1"
  
  developer_connect_event_config {
    git_repository_link = "projects/{{index $.TestEnvVars "project_name"}}/locations/{{index $.TestEnvVars "location"}}/connections/{{index $.TestEnvVars "connection_name"}}/gitRepositoryLinks/{{index $.TestEnvVars "repo_name"}}"
    push {
      branch = "feature-.*"
    }
  }

  filename = "cloudbuild.yaml"
}
