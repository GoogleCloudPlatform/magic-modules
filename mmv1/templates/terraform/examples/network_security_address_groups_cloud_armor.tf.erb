resource "google_network_security_address_group" "<%= ctx[:primary_resource_id] %>" {
  provider    = google-beta
  name        = "<%= ctx[:vars]['resource_name'] %>"
  parent      = "projects/<%= ctx[:test_env_vars]['project'] %>"
  location    = "global"
  type        = "IPV4"
  capacity    = "100"
  purpose     = ["CLOUD_ARMOR"]
  items       = ["208.80.154.224/32"]
}
