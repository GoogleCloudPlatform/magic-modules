resource "google_firestore_database" "<%= ctx[:primary_resource_id] %>" {
  parent      = "projects/<%= ctx[:test_env_vars]['project'] %>"
  database_id = "(default)"
  name        = "projects/<%= ctx[:test_env_vars]['project'] %>/databases/(default)"
  type        = "FIRESTORE_NATIVE"
  location_id = "us-west1"
}

