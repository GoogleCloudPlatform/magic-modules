resource "google_clouddomains_registration" <%= ctx[:primary_resource_id] %> {
  domain_name = "tf-test-domain.com"
  location    = "global"
  labels = {
    labelkey = "labelvalue"
  }
  domain_notices  = ["HSTS_PRELOADED"]
  contact_notices = ["PUBLIC_CONTACT_DATA_ACKNOWLEDGEMENT"]
  yearly_price {
    currency_code = "USD"
    units         = 12
  }
  dns_settings {
    google_domains_dns {
      ds_state = "DS_RECORDS_PUBLISHED"
    }
  }
  contact_settings {
    privacy = "PUBLIC_CONTACT_DATA"
    registrant_contact {
      phone_number = "+12345000000"
      email        = "user@example.com"
      postal_address {
        region_code         = "US"
        postal_code         = "12345"
        administrative_area = "CA"
        locality            = "Example City"
        address_lines       = ["1234 Example street"]
        recipients          = ["example"]
      }
    }
    admin_contact {
      phone_number = "+12345000000"
      email        = "user@example.com"
      postal_address {
        region_code         = "US"
        postal_code         = "12345"
        administrative_area = "CA"
        locality            = "Example City"
        address_lines       = ["1234 Example street"]
        recipients          = ["example"]
      }
    }
    technical_contact {
      phone_number = "+12345000000"
      email        = "user@example.com"
      postal_address {
        region_code         = "US"
        postal_code         = "12345"
        administrative_area = "CA"
        locality            = "Example City"
        address_lines       = ["1234 Example street"]
        recipients          = ["example"]
      }
    }
  }
}
