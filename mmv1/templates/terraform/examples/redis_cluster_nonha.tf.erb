resource "google_redis_cluster" "<%= ctx[:primary_resource_id] %>" {
  provider = google-beta
  name           = "<%= ctx[:vars]['cluster_name'] %>"
  shard_count    = 3
  psc_configs = [
    {
      network = data.google_compute_network.redis-network.id
    }
  ]
}

data "google_compute_network" "redis-network" {
  name = "projects/${data.google_project.project.project_id}/global/networks/default"
}

data "google_project" "project" {
}
