<%
	resource_type = property.resource_ref.base_url.split('/').last
-%>
<% if property.resource_ref.base_url.include?('{{region}}') -%>
tpgresource.ParseRegionalFieldValue("<%= resource_type -%>", <%= var_name -%>, "project", "region", "zone", d, config, true)
<% elsif property.resource_ref.base_url.include?('{{zone}}') -%>
tpgresource.ParseZonalFieldValue("<%= resource_type -%>", <%= var_name -%>, "project", "zone", d, config, true)
<% else -%>
tpgresource.ParseGlobalFieldValue("<%= resource_type -%>", <%= var_name -%>, "project", d, config, true)
<% end -%>
