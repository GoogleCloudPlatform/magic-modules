idParts := strings.Split(d.Id(), "/")
if len(idParts) != 7 {
  return fmt.Errorf("unexpected format of ID (%q), expected folders/{folder}/locations/{location}/notificationConfigs/{config_id}", d.Id())
}

if err := d.Set("folder", idParts[1]); err != nil {
  return fmt.Errorf("error setting folder: %s", err)
}

if err := d.Set("config_id", idParts[5]); err != nil {
  return fmt.Errorf("error setting config_id: %s", err)
}
