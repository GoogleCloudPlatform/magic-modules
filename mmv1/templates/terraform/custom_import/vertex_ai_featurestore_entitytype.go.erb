	config := meta.(*Config)

	if err := parseImportId([]string{
		"projects/(?P<project>[^/]+)/locations/(?P<location>[^/]+)/featurestores/(?P<featurestore>[^/]+)/entityTypes/(?P<name>[^/]*)",
		"(?P<project>[^/]+)/(?P<location>[^/]+)/(?P<featurestore>[^/]+)/(?P<name>[^/]+)",
		"(?P<name>[^/]+)",
	}, d, config); err != nil {
		return nil, err
	}

	location := d.Get("location").(string)

	if err := d.Set("region", location); err != nil {
		return nil, fmt.Errorf("Error setting region: %s", err)
	}

	return []*schema.ResourceData{d}, nil