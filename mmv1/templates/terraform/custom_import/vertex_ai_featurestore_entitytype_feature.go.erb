config := meta.(*Config)
if err := parseImportId([]string{
	"(?P<entitytype>.+)/features/(?P<name>[^/]+)",
}, d, config); err != nil {
	return nil, err
}

// Replace import id for the resource id
id, err := replaceVars(d, config, "{{entitytype}}/features/{{name}}")
if err != nil {
	return nil, fmt.Errorf("Error constructing id: %s", err)
}
d.SetId(id)

entitytype := d.Get("entitytype").(string)

re := regexp.MustCompile("^projects/(.+)/locations/(.+)/featurestores/(.+)$")
if parts := re.FindStringSubmatch(entitytype); parts != nil {
	d.Set("region", parts[2])
}

return []*schema.ResourceData{d}, nil
