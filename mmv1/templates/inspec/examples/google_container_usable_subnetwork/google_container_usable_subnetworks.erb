<% gcp_project_id = "#{external_attribute(pwd, 'gcp_project_id', doc_generation)}" -%>
<% usable_subnetwork = grab_attributes(pwd)['usable_subnetwork'] -%>
describe google_container_usable_subnetworks(project: <%= gcp_project_id -%>) do
  its('network') { should include <%= doc_generation ? "'#{usable_subnetwork['network']}'" : "usable_subnetwork['network']" -%> }
  its('ipCidrRange') { should include <%= doc_generation ? "'#{usable_subnetwork['ipCidrRange']}'" : "usable_subnetwork['ipCidrRange']" -%> }
end