<% gcp_project_id = "#{external_attribute(pwd, 'gcp_project_id', doc_generation)}" -%>
<% gcp_location = "#{external_attribute(pwd, 'gcp_location', doc_generation)}" -%>
<% region_instance_group = grab_attributes(pwd)['region_instance_group'] -%>
describe google_compute_region_instance_groups(project: <%= gcp_project_id -%>, region: <%= doc_generation ? "'#{region_instance_group['region']}'" : "region_instance_group['region']" -%>) do
	it { should exist }
	its('names') { should include <%= doc_generation ? "'#{region_instance_group['name']}'" : "region_instance_group['name']" -%> }
end