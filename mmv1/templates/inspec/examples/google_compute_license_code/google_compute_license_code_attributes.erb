gcp_project_id = attribute(:gcp_project_id, default: '<%= external_attribute(pwd, 'gcp_project_id') -%>', description: 'The GCP project identifier.')
license_code = attribute('license_code', default: <%= JSON.pretty_generate(grab_attributes(pwd)['license_code']) -%>, description: 'License codes are mirrored across all projects that have permissions to read the License Code.')