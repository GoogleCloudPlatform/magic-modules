<% gcp_project_id = "#{external_attribute(pwd, 'gcp_project_id', doc_generation)}" -%>
<% license_code = grab_attributes(pwd)['license_code'] -%>
describe google_compute_license_code(project: <%= doc_generation ? "#{gcp_project_id}" : "gcp_project_id" -%>, name: <%= doc_generation ? "'#{license_code['name']}'" : "license_code['name']" -%>) do
  it { should exist }
end

describe google_compute_license_code(project: <%= doc_generation ? "#{gcp_project_id}" : "gcp_project_id" -%>, name: 'nonexistent') do
  it { should_not exist }
end