<% gcp_project_id = "#{external_attribute(pwd, 'gcp_project_id', doc_generation)}" -%>
<% interconnect_location = grab_attributes(pwd)['interconnect_location'] -%>
describe google_compute_interconnect_locations(project: <%= doc_generation ? "#{gcp_project_id}" : "gcp_project_id" -%>) do
  its('names') { should include <%= doc_generation ? "'#{interconnect_location['name']}'" : "interconnect_location['name']" -%> }
  its('facility_provider_facility_ids') { should include <%= doc_generation ? "'#{interconnect_location['facility_provider_facility_id']}'" : "interconnect_location['facility_provider_facility_id']" -%> }
end