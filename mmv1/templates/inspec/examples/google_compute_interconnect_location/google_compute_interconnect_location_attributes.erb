gcp_project_id = attribute(:gcp_project_id, default: '<%= external_attribute(pwd, 'gcp_project_id') -%>', description: 'The GCP project identifier.')
interconnect_location = attribute('interconnect_location', default: <%= JSON.pretty_generate(grab_attributes(pwd)['interconnect_location']) -%>, description: 'Retrieves the list of interconnect locations available to the specified project.')