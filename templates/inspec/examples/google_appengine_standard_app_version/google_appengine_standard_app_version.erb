<% gcp_project_id = "#{external_attribute('gcp_project_id', doc_generation)}" -%>
<% gcp_standardappversion_region = "#{external_attribute('gcp_standardappversion_region', doc_generation)}" -%>
<% standardappversion = grab_attributes['standardappversion'] -%>

describe google_appengine_standard_app_version(project: <%= gcp_project_id -%>, location: <%= gcp_standardappversion_region -%>, version_id: <%= doc_generation ? "'#{standardappversion['version_id']}'" : "standardappversion['version_id']" -%>, service: <%= doc_generation ? "'#{standardappversion['service']}'" : "standardappversion['service']" -%>) do
  it { should exist }
  its('service') { should eq <%= doc_generation ? "'#{standardappversion['service']}'" : "standardappversion['service']" -%> }
  its('name') { should eq <%= doc_generation ? "'#{standardappversion['name']}'" : "standardappversion['name']" -%> }
  its('id') { should eq <%= doc_generation ? "'#{standardappversion['version_id']}'" : "standardappversion['version_id']" -%> }
  its('threadsafe') { should be <%= doc_generation ? "'#{standardappversion['threadsafe']}'" : "standardappversion['threadsafe']" -%> }
  its('libraries') { should eq <%= doc_generation ? "'#{standardappversion['libraries']}'" : "standardappversion['libraries']" -%> }
  its('env_variables') { should eq <%= doc_generation ? "'#{standardappversion['env_var_value']}'" : "standardappversion['env_var_value']" -%> }
  its('runtime_api_version') { should eq <%= doc_generation ? "'#{standardappversion['runtime_api_version']}'" : "standardappversion['runtime_api_version']" -%> }
  its('runtime') { should eq <%= doc_generation ? "'#{standardappversion['runtime']}'" : "standardappversion['runtime']" -%> }
  its('entrypoint') { should eq <%= doc_generation ? "'#{standardappversion['entrypoint']}'" : "standardappversion['entrypoint']" -%> }
end