<% gcp_project_id = "#{external_attribute('gcp_project_id', doc_generation)}" -%>
<% spannerdatabase = grab_attributes['spannerdatabase'] -%>

describe google_spanner_databases(project: <%= gcp_project_id -%>, name: <%= doc_generation ? "'#{spannerdatabase['name']}'" : "spannerdatabase['name']" -%>) do
  it { should exist }
  its('name') { should eq <%= doc_generation ? "'#{spannerdatabase['name']}'" : "spannerdatabase['name']" -%> }
end