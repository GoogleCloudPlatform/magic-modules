config := meta.(*Config)
project, err := getProject(d, config)
if err != nil {
  return nil, err
}

zone := d.Get("zone").(string)
region := zone[:len(zone)-2]

obj["resourcePolicies"] = []interface{}{fmt.Sprintf("projects/%s/regions/%s/resourcePolicies/%s", project, region, obj["name"])}
delete(obj, "name")
return obj, nil
