// This decoder is called multiple times on the same object, so it must check if it has already been decoded
// member is only set during decoding, so check it
if _, ok := res["member"]; ok {
	return res, nil
}

role := res["role"]

var member string
if v, ok := res["user_by_email"]; ok {
	member = v.(string)
}

return map[string]interface{}{"member": member, "role": role.(string)}, nil