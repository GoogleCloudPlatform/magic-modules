	config := meta.(*Config)
	url, err := replaceVars(d, config, fmt.Sprintf("{{FirebaseBasePath}}%s/config", res["name"]))
	if err != nil {
		return nil, err
	}

	project, err := getProject(d, config)
	if err != nil {
		return nil, err
	}
	configRes, err := sendRequest(config, "GET", project, url, nil)
	if err != nil {
		return nil, handleNotFoundError(err, d, fmt.Sprintf("FirebaseWebApp config %q", d.Id()))
	}
	res["config"] = configRes

return res, nil
