config := meta.(*Config)

selfLink, ok := res["selfLink"]
if !ok {
	return res, nil
}
url := fmt.Sprintf("%s/listNetworkEndpoints", selfLink)

project, err := getProject(d, config)
if err != nil {
	return nil, err
}
endpointsRes, err := sendRequest(config, "POST", project, url, nil)
if err != nil {
	return nil, handleNotFoundError(err, d, fmt.Sprintf("ComputeGlobalNetworkEndpoint %q", d.Id()))
}

items, ok := endpointsRes["items"]
if !ok {
	return res, nil
}

itemArray := items.([]interface{})
if len(itemArray) == 0 {
	return res, nil
}
// Global endpoint groups are unable to have more than 1 endpoint
if len(itemArray) > 1 {
	return nil, fmt.Errorf("Unable to decode global network endpoint. Found more than 1 endpoint for the group: %s", selfLink)
}

endpoint := itemArray[0].(map[string]interface{})["networkEndpoint"].(map[string]interface{})
res["fqdn"] = endpoint["fqdn"]
res["port"] = endpoint["port"]
res["ipAddress"] = endpoint["ipAddress"]
return res, nil