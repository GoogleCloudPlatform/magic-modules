resource "google_container_cluster" "<%= ctx[:primary_resource_id] %>" {
  name               = "<%= ctx[:vars]['gke_cluster_name'] %>"
  location           = "us-central1-a"
  initial_node_count = 3
  notification_config {
    pubsub {
      enabled = true
      topic = "projects/{project}/topics/{topic}"  
    }
  }
}
