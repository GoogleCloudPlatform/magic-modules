resource "google_filestore_instance" "<%= ctx[:primary_resource_id] %>" {
  provider = "google-beta"

  name = "<%= ctx[:vars]["instance_name"] %>"
  zone = "us-central1-b"
  tier = "PREMIUM"

  file_shares {
    capacity_gb = 2660
    name        = "share1"
  }

  networks {
    network = "default"
    modes   = ["MODE_IPV4"]
  }
}

provider "google-beta"{
  region = "us-central1"
  zone   = "us-central1-a"
}
