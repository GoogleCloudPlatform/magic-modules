resource "google_ml_engine_job" "<%= ctx[:primary_resource_id] -%>" {
  job_id = "ml_job_tesnor"

  training_input {
    job_dir         = "<%= ctx[:vars]['gcs_job_dir'] -%>"
    package_uris    = [""<%= ctx[:vars]['gcs_pkg_uri'] -%>""]
    args            = ["--train-files=gs://workingnow/data/adult.data.csv", "--eval-files=gs://workingnow/data/adult.test.csv", "--train-steps=1000", "--eval-steps=100"]
    python_module   = "trainer.task"
    region          = "us-central1"
    runtime_version = "1.14"
    scale_tier      = "BASIC"
  }
}
