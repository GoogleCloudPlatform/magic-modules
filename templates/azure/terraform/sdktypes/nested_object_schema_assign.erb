<%
    sdk_type = sdk_marshal.sdktype.type_definition
    temp_var = sdk_type.go_variable_name || sdk_type.go_field_name.camelcase(:lower) || input
-%>
if <%= temp_var -%> := <%= input -%>.<%= sdk_type.go_field_name -%>; <%= temp_var -%> != nil {
<%= lines(build_sdk_object_to_property(temp_var, output, sdk_marshal.clone())) -%>
}