<%
  provider_helpers = Google::HashUtils.navigate(config,
                                   %w[provider_helpers include], [])
-%>
<% unless provider_helpers.empty? -%>
<%=
  provider_helpers.map do |f|
    if f == provider_helpers.last
      lines(compile(f))
    else
      lines(compile(f), 2)
    end
  end.join
-%>
<% end -%>
