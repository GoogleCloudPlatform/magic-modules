<%# Requires: none -%>
<%
  test_resourcerefs_for_properties(object.all_user_properties,
                                   object).each do |ref|
-%>
<%
  prop_ref = ref.resources.first.resource_ref
  ref_underscore_name = Google::StringUtils.underscore(ref.resources
                                                          .first.resource)
-%>
def expand_variables_<%= ref_underscore_name -%>(template, data, ext_dat = {})
<% prefix = object.__product.prefix.upcase -%>
  Google::<%= prefix -%>::<%= prop_ref.name %>
    .action_class.expand_variables(template, data, ext_dat)
end

<% end -%>
